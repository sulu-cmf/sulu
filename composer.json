{
    "name": "sulu/sulu",
    "license": "MIT",
    "type": "library",
    "description": "Core framework that implements the functionality of the Sulu content management system.",
    "homepage": "https://sulu.io/",
    "keywords": [
        "symfony",
        "sulu",
        "sulucms",
        "cms",
        "web-application",
        "platform",
        "data-managment"
    ],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/sulu/sulu/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/sulu/skeleton/issues",
        "source": "https://github.com/sulu/skeleton",
        "docs": "http://docs.sulu.io/"
    },
    "require": {
        "php": "^7.2",
        "aferrandini/urlizer": "^1.0",
        "dantleech/phpcr-migrations-bundle": "^1.0",
        "doctrine/annotations": "^1.2",
        "doctrine/common": "^2.7.1",
        "doctrine/data-fixtures": "^1.1",
        "doctrine/dbal": "^2.6",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-cache-bundle": "^1.3.1",
        "doctrine/doctrine-fixtures-bundle": "^2.4 || ^3.0",
        "doctrine/inflector": "^1.3",
        "doctrine/orm": "^2.5.3",
        "doctrine/phpcr-bundle": "^1.3.5 || ^2.0",
        "friendsofsymfony/http-cache": "^2.5",
        "friendsofsymfony/http-cache-bundle": "^2.6",
        "friendsofsymfony/jsrouting-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "goodby/csv": "^1.3",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.5",
        "imagine/imagine": "^0.6.1 || ^0.7.0 || ^1.0",
        "jms/serializer": "^3.0",
        "jms/serializer-bundle": "^3.0",
        "layershifter/tld-extract": "^1.1 || ^2.0",
        "massive/build-bundle": "^0.3 || ^0.4",
        "massive/search-bundle": "^2.0.0",
        "mtdowling/cron-expression": "^1.1",
        "ocramius/proxy-manager": "^2.1",
        "oro/doctrine-extensions": "^1.0",
        "pagerfanta/pagerfanta": "^1.0.4 || ^2.0",
        "php-http/client-common": "^1.1.0 || ^2.0",
        "php-http/guzzle6-adapter": "^1.1 || ^2.0",
        "piwik/device-detector": "^3.7",
        "ramsey/uuid": "^3.1",
        "stof/doctrine-extensions-bundle": "^1.2.2",
        "symfony-cmf/routing-bundle": "^2.1",
        "symfony-cmf/slugifier-api": "^2.0",
        "symfony/asset": "^4.3",
        "symfony/cache": "^4.3",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/dom-crawler": "^4.3",
        "symfony/event-dispatcher": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3.1",
        "symfony/http-client": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/intl": "^4.3",
        "symfony/options-resolver": "^4.3",
        "symfony/polyfill-mbstring": "^1.0",
        "symfony/polyfill-php72": "^1.0",
        "symfony/property-access": "^4.3",
        "symfony/proxy-manager-bridge": " ^4.3",
        "symfony/routing": "^4.3",
        "symfony/security": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/swiftmailer-bundle": "^3.1.4",
        "symfony/templating": "^4.3",
        "symfony/translation": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/yaml": "^4.3",
        "toflar/psr6-symfony-http-cache-store": "^1.1 || ^2.0",
        "twig/twig": "^1.41 || ^2.0"
    },
    "require-dev": {
        "google/cloud-storage": "^1.0",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0.1",
        "matthiasnoback/symfony-config-test": "^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "php-ffmpeg/php-ffmpeg": "^0.13 || ^0.14",
        "phpcr/phpcr-shell": "^1.0",
        "phpspec/prophecy": "^1.7",
        "phpstan/phpstan": "^0.11.5",
        "phpstan/phpstan-symfony": "^0.11.5",
        "phpunit/phpunit": "^8.0",
        "superbalist/flysystem-google-storage": "^7.1",
        "symfony/browser-kit": "^4.3",
        "symfony/debug": "^4.3",
        "symfony/debug-bundle": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/process": "^4.3",
        "symfony/stopwatch": "^4.3",
        "symfony/var-dumper": "^4.3",
        "symfony/web-profiler-bundle": "^4.3",
        "symfony/web-server-bundle": "^4.3",
        "zendframework/zend-stdlib": "^2.3",
        "zendframework/zendsearch": "@dev"
    },
    "conflict": {
        "doctrine/dbal": "2.7.0",
        "doctrine/lexer": "1.0.0",
        "symfony/doctrine-bridge": "< 3.4.0",
        "symfony/http-foundation": "4.3.4",
        "jackalope/jackalope": "< 1.3.4",
        "jackalope/jackalope-jackrabbit": "< 1.3.0",
        "jackalope/jackalope-doctrine-dbal": "< 1.3.0",
        "phpcr/phpcr-utils": "1.2.0 - 1.2.10 || 1.3.0 - 1.3.2"
    },
    "replace": {
        "sulu/document-manager": "self.version",
        "sulu/media-bundle": "self.version",
        "sulu/tag-bundle": "self.version",
        "sulu/category-bundle": "self.version",
        "sulu/test-bundle": "self.version",
        "sulu/search-bundle": "self.version",
        "sulu/security-bundle": "self.version",
        "sulu/website-bundle": "self.version",
        "sulu/admin-bundle": "self.version",
        "sulu/contact-bundle": "self.version",
        "sulu/snippet-bundle": "self.version",
        "sulu/location-bundle": "self.version",
        "sulu/translate-bundle": "self.version"
    },
    "suggest": {
        "league/flysystem": "^1.0 - Needed for remote media-storages",
        "league/flysystem-aws-s3-v3": "^1.0.1 - Needed for AWS S3 compatible storages (aws s3 or minio)",
        "superbalist/flysystem-google-storage": "^7.1 - Needed for Google-Bucket media-storage implementation",
        "php-ffmpeg/php-ffmpeg": "Needed (as in require-dev) to generate preview images for videos",
        "symfony/monolog-bundle": "^3.1",
        "zendframework/zend-stdlib": "Needed (as in require dev) in addition to zendsearch",
        "zendframework/zendsearch": "To use the PHP based Zend Search library (based on Lucene)"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Sulu/Component/*/Tests/",
            "src/Sulu/Bundle/*/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.6": "1.6.x-dev",
            "dev-release/2.0": "2.0.x-dev",
            "dev-master": "2.1.x-dev"
        }
    }
}
