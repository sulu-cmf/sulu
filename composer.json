    "description": "Symfony bundle that provides basic core functionality",
    "license": "MIT",
    "keywords": ["core", "sulu"],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/massiveart/SuluContactBundle/contributors"
        }
