<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Sulu\Bundle\ContactBundle\Entity\EmailType" table="co_email_types">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="name" type="string" column="name" length="60"/>
        <one-to-many field="emails" target-entity="Sulu\Bundle\ContactBundle\Entity\Email" mapped-by="emailType"/>
    </entity>
</doctrine-mapping>
