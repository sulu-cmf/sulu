<class xmlns="http://schemas.sulu.io/class/general"
       xmlns:orm="http://schemas.sulu.io/class/doctrine"
       xmlns:list="http://schemas.sulu.io/class/list">
    <properties>
        <property
            name="id"
            visibility="always"
            list:sortable="false"
            list:translation="sulu_admin.id"
        />
        <property
            name="title"
            visibility="always"
            list:translation="sulu_content.title"
        />
        <property
            name="order"
            visibility="always"
            list:translation="sulu_content.order"
        />
        <property
            name="published"
            list:type="datetime"
            visibility="always"
            list:translation="sulu_content.published"
        />
    </properties>
</class>
