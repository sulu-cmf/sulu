<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>snippets</key>

    <properties>
        <property
            name="id"
            visibility="no"
            filter-type="string"
            translation="sulu_admin.id"
        />

        <property
            name="title"
            visibility="always"
            searchability="yes"
            filter-type="string"
            translation="sulu_admin.title"
        />

        <property
            name="template"
            visibility="yes"
            translation="sulu_admin.type"
        />

        <property
            name="created"
            visibility="no"
            translation="sulu_admin.created"
        />

        <property
            name="changed"
            visibility="no"
            translation="sulu_admin.changed"
        />
    </properties>
</list>
