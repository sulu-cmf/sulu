<url>
    <loc>{{ location }}</loc>
    {% if entry.lastmod %}
    <lastmod>{{ entry.lastmod|date('Y-m-d') }}</lastmod>
    {% endif %}
    {% if entry.changefreq %}
    <changefreq>{{ entry.changefreq }}</changefreq>
    {% endif %}
    {% if entry.priority %}
    <priority>{{ entry.priority }}</priority>
    {% endif %}

    {% include 'SuluWebsiteBundle:Sitemap:alternate-links.xml.twig' %}
</url>
