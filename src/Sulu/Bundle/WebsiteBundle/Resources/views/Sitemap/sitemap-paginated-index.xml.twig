{% spaceless %}
<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in range(1, maxPage) %}
    <sitemap>
        <loc>{{ url('sulu_website.paginated_sitemap', {alias: alias, page: page}) }}</loc>
    </sitemap>
    {% endfor %}
</sitemapindex>
{% endspaceless %}
