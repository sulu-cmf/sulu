<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>account_contacts</key>

    <properties>
        <property
            name="fullName"
            searchability="yes"
            sortable="true"
            translation="sulu_contact.name"
            visibility="always"
        />

        <property
            name="position"
            searchability="no"
            sortable="true"
            translation="sulu_contact.position"
            visibility="yes"
        />
    </properties>
</list>
