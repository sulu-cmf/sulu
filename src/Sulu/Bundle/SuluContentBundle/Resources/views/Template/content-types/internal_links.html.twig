<div id="{{ id|raw }}"
     data-property='{{ property|json_encode }}'
     data-mapper-property="{{ property.name }}"
     data-form="true"
     data-type="internalLinks"
     data-type-instance-name="{{ id|raw }}"
     data-aura-component="internal-links@sulucontent"
     data-aura-instance-name="{{ id|raw }}"
     data-aura-url="/admin/api/nodes?language={{ languageCode }}&webspace={{ webspaceKey }}&flat=true"
     data-aura-result-key="nodes"
     data-aura-column-navigation-url="/admin/api/nodes?tree=true&webspace={{ webspaceKey }}&language={{ languageCode }}&webspace-node=true">
</div>
