<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>pages</key>

    <properties>
        <property
            name="id"
            visibility="no"
            sortable="false"
            translation="sulu_admin.id"
        />
        <property
            name="title"
            visibility="always"
            translation="sulu_admin.title"
        />
        <property
            name="published"
            type="datetime"
            visibility="yes"
            translation="sulu_admin.published"
        />
    </properties>
</list>
