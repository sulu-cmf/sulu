<?xml version="1.0" ?>
<datagrid xmlns="http://schemas.sulu.io/list-builder/datagrid">
    <key>pages</key>

    <properties>
        <property
            name="id"
            visibility="always"
            sortable="false"
            translation="sulu_admin.id"
        />
        <property
            name="title"
            visibility="always"
            translation="sulu_admin.title"
        />
        <property
            name="order"
            visibility="always"
            translation="sulu_page.order"
        />
        <property
            name="published"
            type="datetime"
            visibility="always"
            translation="sulu_page.published"
        />
    </properties>
</datagrid>
