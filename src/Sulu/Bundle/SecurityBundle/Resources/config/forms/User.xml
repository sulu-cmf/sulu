<?xml version="1.0" ?>
<properties xmlns="http://schemas.sulu.io/template/template"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://schemas.sulu.io/template/template http://schemas.sulu.io/template/properties-1.0.xsd">
    <property name="username" type="text_line" mandatory="true" colspan="6" spaceAfter="6">
        <meta>
            <title lang="en">Username</title>
            <title lang="de">Username</title>
        </meta>
    </property>

    <property name="password" type="password_confirmation" mandatory="true">
        <meta>
            <title lang="en">Password</title>
            <title lang="de">Passwort</title>
        </meta>
    </property>
</properties>
